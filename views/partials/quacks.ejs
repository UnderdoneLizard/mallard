<!-- Bootstrap CSS -->
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
<% quacks.forEach( quack => { %>
    
    <div class="card shadow-sm p-3 mb-5 bg-white rounded">
        <div class="card-body">
            <a href="/user/<%= quack.user.id %>">
                <h5 class="card-title"><%= quack.user.displayName %></h5>
                <h6 class="card-subtitle">@<%= quack.user.username %></h6>
            </a>
            <a href="/quack/<%= quack.id %>"><p class="card-text"><%= quack.content %> </p></a>
            <% if (quack.likes) { %>
                <a href="/quack/<%= quack.id %>/likes"><p class="card-link"><%= quack.likes.length %> likes </p></a>
                <% } %>
            <% if (quack.dislikes) { %>
                <a href="/quack/<%= quack.id %>/dislikes"><p class="card-link"><%= quack.dislikes.length %> dis-likes </p></a>
            <% } %>
        </div>
    </div>
    
<% }) %> 